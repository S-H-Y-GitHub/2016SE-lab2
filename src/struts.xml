<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <include file="struts-default.xml"/>
    <package name="Book_Management" extends="struts-default">
        <action name="List" class="zzy.Action" method="list">
            <result>list.jsp</result>
        </action>
        <action name="Edit" class="zzy.Action" method="load">
            <result>Edit.jsp</result>
        </action>
        <action name="Store" class="zzy.Action" method="store">
            <result type="redirect">List.action</result>
        </action>
        <action name="Remove" class="zzy.Action" method="remove">
            <result type="redirect">List.action</result>
        </action>
        <action name="searchAuthor" class="zzy.Action" method="searchAuthor">
            <result>list.jsp</result>
        </action>
    </package>
</struts>